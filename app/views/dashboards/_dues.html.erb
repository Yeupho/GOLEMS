<div class="panel panel-default shadow-pane">
  <div class="panel-heading">
    <h2 class="panel-title">Unpaid Dues</h2>
  </div>
  <div class="panel-body">

    <div class="imaginary_container">
      <table class="table table-hover">
        <thead>
        <tr>
          <th>Name</th>
          <th>Phone</th>
          <th>Due Date</th>
          <th>Event</th>
          <th>Dues</th>
          <th>Action</th>
        </tr>
        </thead>

        <tbody>
        <% @check_in.each do |check_in| %>
            <tr data-toggle="collapse" data-target="#accordion" class="clickable">
              <td><%= check_in.customer.first_name %> <%= check_in.customer.last_name %></td>
              <td><%= number_to_phone(check_in.customer.phone, area_code: true) %></td>
              <td><%= check_in.event.event_date.strftime("%-m/%-d") %></td>
              <td><%= check_in.event.event_name %></td>
              <td></td>
              <td></td>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>

  </div>
</div>